<h1>Listing assists</h1>

<table>
  <thead>
    <tr>
      <th>Game</th>
      <th>Atime</th>
      <th>Player</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @assists.each do |assist| %>
      <tr>
        <td><%= assist.game %></td>
        <td><%= assist.aTime %></td>
        <td><%= assist.player %></td>
        <td><%= link_to 'Show', assist %></td>
        <td><%= link_to 'Edit', edit_assist_path(assist) %></td>
        <td><%= link_to 'Destroy', assist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Assist', new_assist_path %>
